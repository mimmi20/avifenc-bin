    "img",
    "jpeg",
    "jpg",
    "png",
    "minify",
    "optimize",
    "libavif",
    "avifenc"
  ],
  "dependencies": {
    "bin-build": "^3.0.0",
    "bin-wrapper": "^4.0.0",
    "logalot": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^3.8.0",
    "bin-check": "^4.0.1",
    "compare-size": "^3.0.0",
    "execa": "^4.0.0",
    "tempy": "^0.5.0",
    "xo": "^0.30.0"
  }
}
